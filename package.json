    "vconsole": "^3.14.7",
    "vite-plugin-theme": "^0.8.6",
    "vue": "^3.2.41",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vuedraggable": "^2.24.3"
  },
